<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.travelappwithlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RestaurantActivity"
            android:label="@string/restaurantactivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".MeuseumActivity"
            android:label="@string/Museumsactivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Historicalsites"
            android:label="@string/historicalsitesactivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ParksActivity"
            android:label="@string/parksactivity"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>